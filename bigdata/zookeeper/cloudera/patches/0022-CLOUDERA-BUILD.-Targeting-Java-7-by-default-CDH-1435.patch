Subject: [PATCH 22/94] CLOUDERA-BUILD. Targeting Java 7 by default
 (CDH-14356)

---
 build.xml |    2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/build.xml b/build.xml
index f89d603..a4a28da 100644
--- a/build.xml
+++ b/build.xml
@@ -56,7 +56,7 @@
     <property name="revision.properties" value="revision.properties" />
     <property file="${basedir}/src/java/${revision.properties}" />
 
-    <property name="targetJavaVersion" value="1.5"/>
+    <property name="targetJavaVersion" value="1.7"/>
     <property name="javac.target" value="${targetJavaVersion}" />
     <property name="javac.source" value="1.5" />
 
